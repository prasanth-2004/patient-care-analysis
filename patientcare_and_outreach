    {
      "cell_type": "code",
      "source": [
        "import streamlit as st\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import hashlib\n",
        "from pymongo import MongoClient\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "from sklearn.svm import SVC\n",
        "from sklearn import preprocessing\n",
        "\n",
        "# ---------------------- DATABASE CONNECTION ----------------------\n",
        "client = MongoClient(\"mongodb+srv://purushoth:purushoth@cluster0.fmkkr.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0\")\n",
        "db = client[\"user_database\"]\n",
        "users_collection = db[\"users\"]\n",
        "\n",
        "# ---------------------- PASSWORD HASHING FUNCTION ----------------------\n",
        "def hash_password(password):\n",
        "    return hashlib.sha256(password.encode()).hexdigest()\n",
        "\n",
        "# ---------------------- USER REGISTRATION FUNCTION ----------------------\n",
        "def register_user(name, dob, email, phone, password):\n",
        "    if users_collection.find_one({\"email\": email}) or users_collection.find_one({\"phone\": phone}):\n",
        "        st.error(\"Email or phone already registered!\")\n",
        "        return False\n",
        "    user_data = {\n",
        "        \"name\": name,\n",
        "        \"dob\": str(dob),\n",
        "        \"email\": email,\n",
        "        \"phone\": phone,\n",
        "        \"password\": hash_password(password)\n",
        "    }\n",
        "    users_collection.insert_one(user_data)\n",
        "    st.success(\"Registration successful! Please login.\")\n",
        "    st.session_state[\"page\"] = \"login\"\n",
        "    st.rerun()\n",
        "\n",
        "# ---------------------- USER LOGIN FUNCTION ----------------------\n",
        "def login_user(username, password):\n",
        "    user = users_collection.find_one({\"$or\": [{\"email\": username}, {\"phone\": username}]})\n",
        "    if user and user.get(\"password\") == hash_password(password):\n",
        "        st.session_state[\"page\"] = \"home\"\n",
        "        st.session_state[\"user_name\"] = user[\"name\"]\n",
        "        st.rerun()\n",
        "    else:\n",
        "        st.error(\"Invalid credentials!\")\n",
        "\n",
        "# ---------------------- LOAD DATASETS & TRAIN MODELS ----------------------\n",
        "@st.cache_data\n",
        "def load_data():\n",
        "    training = pd.read_csv(\"Training.csv\")\n",
        "    testing = pd.read_csv(\"Testing.csv\")\n",
        "    severity = pd.read_csv(\"Symptom-severity.csv\")\n",
        "    description = pd.read_csv(\"symptom_Description.csv\")\n",
        "    precautions = pd.read_csv(\"symptom_precaution.csv\")\n",
        "\n",
        "    return training, testing, severity, description, precautions\n",
        "\n",
        "training, testing, severity, description, precautions = load_data()\n",
        "cols = training.columns[:-1]\n",
        "X = training[cols]\n",
        "y = training['prognosis']\n",
        "\n",
        "le = preprocessing.LabelEncoder()\n",
        "y = le.fit_transform(y)\n",
        "\n",
        "# Train Decision Tree model\n",
        "clf = DecisionTreeClassifier()\n",
        "clf.fit(X, y)\n",
        "\n",
        "# Train SVM model\n",
        "svm_model = SVC()\n",
        "svm_model.fit(X, y)\n",
        "\n",
        "# ---------------------- LOGIN PAGE ----------------------\n",
        "def login_page():\n",
        "    st.title(\"🔐 Login\")\n",
        "    username = st.text_input(\"Email / Phone\", key=\"login_username\")\n",
        "    password = st.text_input(\"Password\", type=\"password\", key=\"login_password\")\n",
        "\n",
        "    if st.button(\"Login\"):\n",
        "        login_user(username, password)\n",
        "\n",
        "    st.write(\"Don't have an account?\")\n",
        "    if st.button(\"Sign Up\"):\n",
        "        st.session_state[\"page\"] = \"signup\"\n",
        "        st.rerun()\n",
        "\n",
        "# ---------------------- SIGNUP PAGE ----------------------\n",
        "def signup_page():\n",
        "    st.title(\"📝 Sign Up\")\n",
        "    name = st.text_input(\"Full Name\", key=\"signup_name\")\n",
        "    dob = st.date_input(\"Date of Birth\", key=\"signup_dob\")\n",
        "    email = st.text_input(\"Email\", key=\"signup_email\")\n",
        "    phone = st.text_input(\"Phone\", key=\"signup_phone\")\n",
        "    password = st.text_input(\"Password\", type=\"password\", key=\"signup_password\")\n",
        "    confirm_password = st.text_input(\"Confirm Password\", type=\"password\", key=\"signup_confirm_password\")\n",
        "\n",
        "    if st.button(\"Register\"):\n",
        "        if password != confirm_password:\n",
        "            st.error(\"Passwords do not match!\")\n",
        "        else:\n",
        "            register_user(name, dob, email, phone, password)\n",
        "\n",
        "    if st.button(\"Back to Login\"):\n",
        "        st.session_state[\"page\"] = \"login\"\n",
        "        st.rerun()\n",
        "\n",
        "# ---------------------- DISEASE PREDICTION FUNCTION ----------------------\n",
        "def predict_disease(symptoms):\n",
        "    input_vector = np.zeros(len(cols))\n",
        "    for symptom in symptoms:\n",
        "        if symptom in cols:\n",
        "            input_vector[cols.get_loc(symptom)] = 1\n",
        "\n",
        "    dt_prediction = le.inverse_transform(clf.predict([input_vector]))[0]\n",
        "    svm_prediction = le.inverse_transform(svm_model.predict([input_vector]))[0]\n",
        "\n",
        "    return dt_prediction, svm_prediction\n",
        "\n",
        "# ---------------------- HOME PAGE ----------------------\n",
        "def home_page():\n",
        "    st.title(\"🏠 Disease Prediction System\")\n",
        "    st.write(f\"Hello, **{st.session_state.get('user_name', 'User')}**!\")\n",
        "\n",
        "    # Select symptoms\n",
        "    selected_symptoms = st.multiselect(\"Select your symptoms:\", cols)\n",
        "\n",
        "    if st.button(\"Predict\"):\n",
        "        if selected_symptoms:\n",
        "            dt_pred, svm_pred = predict_disease(selected_symptoms)\n",
        "            st.subheader(\"🩺 Predictions:\")\n",
        "            st.write(f\"**Decision Tree Prediction:** {dt_pred}\")\n",
        "            st.write(f\"**SVM Prediction:** {svm_pred}\")\n",
        "\n",
        "            # Get description and precautions\n",
        "            disease_info = description[description[\"Disease\"] == dt_pred]\n",
        "            if not disease_info.empty:\n",
        "                st.subheader(\"📌 Disease Description:\")\n",
        "                st.write(disease_info[\"Description\"].values[0])\n",
        "\n",
        "            precaution_info = precautions[precautions[\"Disease\"] == dt_pred]\n",
        "            if not precaution_info.empty:\n",
        "                st.subheader(\"🛑 Precautions:\")\n",
        "                st.write(\"\\n\".join(precaution_info.iloc[0, 1:].dropna().values))\n",
        "\n",
        "        else:\n",
        "            st.warning(\"Please select at least one symptom.\")\n",
        "\n",
        "    if st.button(\"Logout\"):\n",
        "        st.session_state[\"page\"] = \"login\"\n",
        "        st.rerun()\n",
        "\n",
        "# ---------------------- NAVIGATION ----------------------\n",
        "if \"page\" not in st.session_state:\n",
        "    st.session_state[\"page\"] = \"login\"\n",
        "\n",
        "if st.session_state[\"page\"] == \"login\":\n",
        "    login_page()\n",
        "elif st.session_state[\"page\"] == \"signup\":\n",
        "    signup_page()\n",
        "elif st.session_state[\"page\"] == \"home\":\n",
        "    home_page()\n"
      ],
