      "cell_type": "code",
      "source": [
        "%%writefile app.py\n",
        "import streamlit as st\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "from sklearn import preprocessing\n",
        "\n",
        "# Load datasets\n",
        "training = pd.read_csv('/content/Training.csv')\n",
        "testing = pd.read_csv('/content/Testing.csv')\n",
        "symptom_desc = pd.read_csv('/content/symptom_Description.csv')\n",
        "symptom_precaution = pd.read_csv('/content/symptom_precaution.csv')\n",
        "severity_data = pd.read_csv('/content/Symptom-severity.csv')\n",
        "\n",
        "# Data preprocessing\n",
        "cols = training.columns[:-1]\n",
        "x = training[cols]\n",
        "y = training['prognosis']\n",
        "le = preprocessing.LabelEncoder()\n",
        "y = le.fit_transform(y)\n",
        "\n",
        "# Train Decision Tree Model\n",
        "clf = DecisionTreeClassifier()\n",
        "clf.fit(x, y)\n",
        "\n",
        "# Streamlit App\n",
        "st.title(\"Patient Care Analysis & Disease Prediction\")\n",
        "\n",
        "# User input for symptoms\n",
        "selected_symptoms = st.multiselect(\"Select Symptoms\", options=cols)\n",
        "\n",
        "if st.button(\"Predict Disease\"):\n",
        "    if selected_symptoms:\n",
        "        # Prepare input\n",
        "        input_data = np.zeros(len(cols))\n",
        "        for symptom in selected_symptoms:\n",
        "            input_data[cols.get_loc(symptom)] = 1\n",
        "\n",
        "        # Predict disease\n",
        "        prediction = clf.predict([input_data])\n",
        "        disease = le.inverse_transform(prediction)[0]\n",
        "\n",
        "        st.success(f\"Predicted Disease: **{disease}**\")\n",
        "\n",
        "        # Display disease description\n",
        "        desc = symptom_desc[symptom_desc['Disease'] == disease]['Description'].values\n",
        "        if desc:\n",
        "            st.info(f\"**Description:** {desc[0]}\")\n",
        "\n",
        "        # Show precautions\n",
        "        precautions = symptom_precaution[symptom_precaution['Disease'] == disease].values\n",
        "        if precautions.size > 0:\n",
        "            st.warning(\"**Precautions:**\")\n",
        "            for i in range(1, 5):\n",
        "                if precautions[0][i]:\n",
        "                    st.write(f\"✔ {precautions[0][i]}\")\n",
        "    else:\n",
        "        st.error(\"Please select at least one symptom.\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "zlRPDencwjDd",
        "outputId": "dbf6319b-0bf8-4824-97b0-812eb33be9bd"
      },
